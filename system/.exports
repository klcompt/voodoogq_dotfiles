export LANG=en_US.UTF-8

#https://github.com/neovim/neovim/issues/5990
export VTE_VERSION="100"

export NET=192.168.1
export TMPDIR=/private$TMPDIR
# Mac LS Colors
#http://superuser.com/questions/417805/how-to-configure-the-ansi-colors-of-the-command-line-terminal
export CLICOLOR=1
export LSCOLORS=ExfxBxDxCxEgEdxbxgxcad

export VISUAL=$(which nvim)
export EDITOR="$VISUAL"

export FZF_DEFAULT_COMMAND='rg --files --follow --glob "!.git/*"'

export GOPATH=$HOME/Code/go
export GOROOT=/usr/local/opt/go/libexec

export NVM_DIR="$HOME/.nvm"
[ -s "/usr/local/opt/nvm/nvm.sh" ] && . "/usr/local/opt/nvm/nvm.sh"  # This loads nvm


# Private export keys
[ -s "$HOME/.private_exports" ] && source "$HOME/.private_exports"

# Logikcull
export FRONTEND_DIR=$HOME/Code/logikdocker/frontend
export BACKEND_DIR=$HOME/Code/logikdocker/backend
